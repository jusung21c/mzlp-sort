					N.SLOT.SCORE.TEMP ~= S.SLOT.SCORE[1][N.TEMP] ;

					IF ( S.SLOT.TAG.POI.CATEGORY = S.SLOT.TAG[1][N.TEMP] ) & ( N.SLOT.SCORE.TEMP > N.SLOT.SCORE.PRE.TEMP ) THEN
