		BEGIN
			MESSAGE "(VH) \fN.GLOBAL.NBEST.RESULT.NUM\fN.BT.CATEGORY.NUM\fN.BT.SELECT.CATEGORY.EXIST\fN.BT.CATEGORY.NUMBER.NUM\fN.BT.NAME.TYPE\fS.BT.NAME.SELECT\fS.BT.NAME.TTS" ;
