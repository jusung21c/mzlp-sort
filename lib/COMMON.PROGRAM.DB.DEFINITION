			MESSAGE "(VH) \fN.BT.DEVICE.PAIRED\fN.BT.DEVICE.CONNECT\fN.SYS.MIRRORLINK.DEVICE.CONNECT\fN.BT.DEVICE.PAIRED.NUM" ;
		END
	IF N.CID = @N_TAG_RADIO_ID THEN ! LpProc.Radio ()
