						BEGIN
							S.SLOT.ID.NUMBER := S.SLOT.ID[1][N.TEMP] ;
							S.SLOT.NUMBER := S.SLOT.VALUE[1][N.TEMP] ;
							S.SLOT.TAG.NUMBER := S.SLOT.TAG[1][N.TEMP] ;
