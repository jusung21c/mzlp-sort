	$N.TRANSITION.DIALOG := @N_DIALOG_EXIT ;
EOP


!!!!!!!!!!!!!!!!!!! 16.12.18 SMART VUI
JINIE Common.ReferenceNBestResultSmartVUIPoiName ()

	TEMPORARY	NUMBER N.INNER.DUPLICATION.COUNT := 0 ; ! DUPLICATION COUNT
	TEMPORARY	NUMBER N.INNER.DUPLICATION.FLAG := 0 ; ! DUPLICATION FLAG

	N.STORE.NBEST := 0 ;
	N.REF.NBEST.COUNT := 1 ;

	S.INNER.LOG.STRING ~= $N.REFERENCE.RESULTS ;
	LOG "LP LOG : Common.ReferenceNBestResultPoiName - $N.REFERENCE.RESULTS = " + S.INNER.LOG.STRING + " | $S.REFERENCE.RESULTS = " + $S.REFERENCE.RESULTS ;

	WHILE ( N.REF.NBEST.COUNT <= $N.RESULT.RESULTS ) & ( N.STORE.NBEST < 4 ) DO
		BEGIN

			N.INNER.DUPLICATION.FLAG := 0 ;
			N.RESULT.ID ~= $S.RESULT.ID[N.REF.NBEST.COUNT][1] ; !1500001

			IF N.RESULT.ID > 1500000 THEN
				BEGIN
					S.RESULT.CID := $S.REFERENCE.RESULTS[1][1] ; ! COMMAND ID
					S.RESULT.CMD := $S.RULE.LEXICONS[N.REF.NBEST.COUNT][2] ; ! COMMAND
					S.RESULT.DIP := $S.RULE.LEXICONS[N.REF.NBEST.COUNT][3] ; ! SCREEN_TEXT
					S.RESULT.SCORE := $S.STT.RESULT.SCORE[N.REF.NBEST.COUNT][1] ; ! SCORE

					S.RESULT.DOMAIN := $S.REFERENCE.RESULTS[1][4] ; ! DOMAIN
					S.RESULT.EXAMPLE := $S.REFERENCE.RESULTS[1][5] ; ! EXAMPLE
					S.RESULT.INTENTION := $S.REFERENCE.RESULTS[1][6] ; ! INTENTION

					S.SLOT.FIRST.ID    := $S.RESULT.ID[N.REF.NBEST.COUNT][1] ; ! Slot1 ID SAVED
					S.SLOT.FIRST.TAG := $S.REFERENCE.RESULTS[1][7] ; ! SLOT1_TAG
					S.SLOT.FIRST.VALUE := $S.RULE.LEXICONS[N.REF.NBEST.COUNT][3] ; ! SLOT1 VALUE SAVED( Display Text )
					S.SLOT.FIRST.SCORE := $S.STT.RESULT.SCORE[N.REF.NBEST.COUNT][1] ; ! SLOT1 SCORE SAVED

					S.SLOT.SECOND.ID := $S.RESULT.ID[N.REF.NBEST.COUNT][1] ; ! Slot2 ID SAVED
					S.SLOT.SECOND.TAG := "<SearchKeyword>" ; ! SLOT2_TAG
					S.SLOT.SECOND.VALUE := $S.RULE.LEXICONS[N.REF.NBEST.COUNT][2] ; ! SLOT2 VALUE SAVED( Search Keyword )
					S.SLOT.SECOND.SCORE := $S.STT.RESULT.SCORE[N.REF.NBEST.COUNT][1] ; ! SLOT2 SCORE SAVED

					S.SLOT.THIRD.ID := "" ; ! Slot3 ID SAVED
					S.SLOT.THIRD.TAG := "_" ; ! SLOT3_TAG
					S.SLOT.THIRD.VALUE := "" ; ! SLOT3 VALUE SAVED
					S.SLOT.THIRD.SCORE := "" ; ! SLOT3 SCORE SAVED

					IF N.REF.NBEST.COUNT = 1 THEN
						BEGIN
							S.RESULT.FIX.CID := S.RESULT.CID ; ! COMMAND ID
							S.RESULT.FIX.CMD := S.RESULT.CMD ; ! COMMAND
							S.RESULT.FIX.DIP := S.RESULT.DIP ; ! SCREEN_TEXT
							S.RESULT.FIX.SCORE := S.RESULT.SCORE ; ! SCORE
						END

					! CALL Common.SetActionString () ;

					N.STORE.NBEST := N.STORE.NBEST + 1 ;
				END
			ELSE !  N.RESULT.ID < 1500000
				BEGIN
					IF $S.RESULT.ID[N.REF.NBEST.COUNT][1] = "\@N_TAG_NAVI_LOCATION_DP_ID" THEN  !  N.RESULT.ID = 690000
						BEGIN
						 S.SMARTVUI.RESULT.ID := "\@N_TAG_NAVI_LOCATION_DP_ID" ;
						 S.SMARTVUI.RESULT.STRING :=  $S.RESULT.STRING[N.REF.NBEST.COUNT][1];
						END
					ELSE
						BEGIN
							IF $S.RESULT.ID[N.REF.NBEST.COUNT][2] = "\@N_TAG_NAVI_LOCATION_DP_ID" THEN !  N.RESULT.ID = 602000 /f 690000
								BEGIN
									S.SMARTVUI.RESULT.ID := "\@N_TAG_NAVI_LOCATION_DP_ID" ;
									S.SMARTVUI.RESULT.STRING :=  $S.RESULT.STRING[N.REF.NBEST.COUNT][2];
								END
						END
					IF S.SMARTVUI.RESULT.ID = "\@N_TAG_NAVI_LOCATION_DP_ID" THEN ! DYNAMIC POI NAME
						BEGIN
							S.RESULT.CID := $S.REFERENCE.RESULTS[1][1] ; ! COMMAND ID
							S.RESULT.CMD := S.SMARTVUI.RESULT.STRING ; ! COMMAND
							S.RESULT.DIP := S.SMARTVUI.RESULT.STRING ; ! SCREEN_TEXT
							S.RESULT.SCORE := $S.STT.RESULT.SCORE[N.REF.NBEST.COUNT][1] ; ! SCORE

							S.RESULT.DOMAIN := $S.REFERENCE.RESULTS[1][4] ; ! DOMAIN
							S.RESULT.EXAMPLE := $S.REFERENCE.RESULTS[1][5] ; ! EXAMPLE
							S.RESULT.INTENTION := $S.REFERENCE.RESULTS[1][6] ; ! INTENTION

							S.SLOT.FIRST.ID    := S.SMARTVUI.RESULT.ID ; ! Slot1 ID SAVED
							S.SLOT.FIRST.TAG := $S.REFERENCE.RESULTS[1][7] ; ! SLOT1_TAG
							S.SLOT.FIRST.VALUE := S.SMARTVUI.RESULT.STRING ; ! SLOT1 VALUE SAVED( Display Text )
							S.SLOT.FIRST.SCORE := $S.STT.RESULT.SCORE[N.REF.NBEST.COUNT][1] ; ! SLOT1 SCORE SAVED

							S.SLOT.SECOND.ID := S.SMARTVUI.RESULT.ID ; ! Slot2 ID SAVED
							S.SLOT.SECOND.TAG := "<SearchKeyword>" ; ! SLOT2_TAG
							S.SLOT.SECOND.VALUE := S.SMARTVUI.RESULT.STRING ; ! SLOT2 VALUE SAVED( Search Keyword )
							S.SLOT.SECOND.SCORE := $S.STT.RESULT.SCORE[N.REF.NBEST.COUNT][1] ; ! SLOT2 SCORE SAVED

							S.SLOT.THIRD.ID := "" ; ! Slot3 ID SAVED
							S.SLOT.THIRD.TAG := "_" ; ! SLOT3_TAG
							S.SLOT.THIRD.VALUE := "" ; ! SLOT3 VALUE SAVED
							S.SLOT.THIRD.SCORE := "" ; ! SLOT3 SCORE SAVED

							IF N.REF.NBEST.COUNT = 1 THEN
								BEGIN
									S.RESULT.FIX.CID := S.RESULT.CID ; ! COMMAND ID
									S.RESULT.FIX.CMD := S.RESULT.CMD ; ! COMMAND
									S.RESULT.FIX.DIP := S.RESULT.DIP ; ! SCREEN_TEXT
									S.RESULT.FIX.SCORE := S.RESULT.SCORE ; ! SCORE
								END

							! CALL Common.SetActionString () ;

							N.STORE.NBEST := N.STORE.NBEST + 1 ;
						END
				END

				IF S.SLOT.FIRST.TAG = "<PoiName>" THEN
					BEGIN
						IF N.REF.NBEST.COUNT = 1 THEN ! MUST EXIST 1-BEST
								BEGIN
									CALL Common.SetActionString () ;
								END
						ELSE
							BEGIN
								! 중복 체크
									N.INNER.DUPLICATION.COUNT := N.REF.NBEST.COUNT - 1 ; ! 현재 Slot String과 이전에 저장 된 Slot String과 비교하기 위하여 N.TEMP - 1 저장

									WHILE 1 <= N.INNER.DUPLICATION.COUNT	DO ! 첫 번째 저장 내용 까지 확인
										BEGIN
											IF S.ACTION.STRING[N.INNER.DUPLICATION.COUNT][6] = S.SLOT.FIRST.VALUE THEN ! POI NAME CHECK
												BEGIN
													N.INNER.DUPLICATION.FLAG := 1 ; ! 중복 시 Flag 값 저장
												END
											N.INNER.DUPLICATION.COUNT := N.INNER.DUPLICATION.COUNT - 1 ; ! 다음 확인을 위하여 - 1
										END

									IF N.INNER.DUPLICATION.FLAG = 0	THEN ! 중복 되지 않을 때
										BEGIN
											CALL Common.SetActionString () ;
										END
							END
					END

			N.REF.NBEST.COUNT := N.REF.NBEST.COUNT + 1 ;
		END

	N.RESULT.CONUT.NUM := N.STORE.NBEST ;

	S.INNER.LOG.STRING ~= N.STORE.NBEST ;
	LOG "LP LOG : N.STORE.NBEST = " + S.INNER.LOG.STRING ;
EOP

