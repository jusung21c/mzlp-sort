! SERVER ENGINE에서 RESULT가 전달되지 않았을 때 수행
JINIE Common.ServerVRExit ()

	! ART에 출력될 최종 RESULT TYPE 전달
	MESSAGE "(INFO) \fTIMEOUT" ;

	LOG "LP LOG : Recognizer - SERVER TIMEOUT/REJECTION !!!" ;
	! FLOW
	$N.CONTROL.FLOW := @N_STAGE_RULE ;

	$S.RESULT.ID := "\@N_TAG_SERVER_INNER_RESULT_ID" ; ! SERVER_TIMEOUT OR SERVER_REJECTION
	$N.RESULT.RESULTS := 1 ;
	$N.RESULT.FORMAT := 2 ;

