! TTS type에 따른 TTS tuning DB 설정
JINIE Common.SetTtsTuningDb (NUMBER N.DIALOG)

	! Default : VR mode
	$S.OUTPUT.RESOURCE.EXPRESSION := "\"DB\"(\"/vr/LPTE/DATA/ENU/apdb_tp_ava_full_enu-vr.dat\") + \"RULE\"(\"/vr/LPTE/DATA/ENU/ruleset_enu.rst\")";

	IF N.DIALOG = @N_DIALOG_VIRTUAL_TTS_BT THEN
		BEGIN
			$S.OUTPUT.RESOURCE.EXPRESSION := "\"DB\"(\"/vr/LPTE/DATA/ENU/apdb_tp_ava_full_enu-vr.dat\") + \"DB\"(\"/vr/LPTE/DATA/ENU/apdb_rp_ava_enu-ava.dat\")";
		END
	IF N.DIALOG = @N_DIALOG_VIRTUAL_TTS_NAVI THEN
		BEGIN
			$S.OUTPUT.RESOURCE.EXPRESSION := "\"DB\"(\"/vr/LPTE/DATA/ENU/apdb_tp_ava_full_enu-nv.dat\") + \"RULE\"(\"/vr/LPTE/DATA/ENU/ruleset_enu.rst\")";
		END
	IF N.DIALOG = @N_DIALOG_VIRTUAL_TTS_SMS THEN
		BEGIN
			$S.OUTPUT.RESOURCE.EXPRESSION := "" ;
		END
	IF N.DIALOG = @N_DIALOG_VIRTUAL_SERVER_DICTATION THEN
		BEGIN
			$S.OUTPUT.RESOURCE.EXPRESSION := "" ;
		END

	LOG "LP LOG : Common.SetTtsTuningDb - $S.OUTPUT.RESOURCE.EXPRESSION = " + $S.OUTPUT.RESOURCE.EXPRESSION ;
	S.INNER.LOG.STRING ~= $N.TTS.PARAMETER.ENGINE.CATEGORY ;
	LOG "LP LOG : Common.SetTtsTuningDb - $N.TTS.PARAMETER.ENGINE.CATEGORY = " + S.INNER.LOG.STRING ;
EOP

