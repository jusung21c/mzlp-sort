			MESSAGE "(VH) \fN.AV.CONDITION\fN.GLOBAL.CONDITION\fN.AVN.BROADCAST.AVAILABLE" ;
		END
	IF N.CID = @N_TAG_MUSIC_ID THEN ! LpProc.Music ()
