			MESSAGE "(VH) \fN.AVN.RADIO.LAST.MODE" ;
		END
	IF N.CID = @N_TAG_MANUAL_QUESTION_MARK_ID | N.CID = @N_TAG_MANUAL_PHONE_QUESTION_MARK_ID | N.CID = @N_TAG_MANUAL_RADIO_QUESTION_MARK_ID | N.CID = @N_TAG_MANUAL_UVO_QUESTION_MARK_ID |
