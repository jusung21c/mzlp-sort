	IF N.CID = @N_TAG_PHONE_MOBILE_ID | N.CID = @N_TAG_PHONE_OFFICE_ID | N.CID = @N_TAG_PHONE_HOME_ID | N.CID = @N_TAG_PHONE_OTHER_ID THEN
