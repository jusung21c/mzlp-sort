			$S.STT.RESULT.ONLINE.SLOT := "" ;
			$S.STT.RESULT.OFFLINE.STRING := "" ;
			$S.STT.RESULT.OFFLINE.DOMAIN := "" ;
			$S.STT.RESULT.OFFLINE.INTENTION := "" ;
			$S.STT.RESULT.OFFLINE.SLOT := "" ;
			$S.ACTION.VALUE := "" ;
			S.ACTION.STRING := "" ;
