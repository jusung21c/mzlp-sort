		N.CID = @N_TAG_REPEAT_ID | N.CID = @N_TAG_REPEAT_OFF_ID THEN
	! LpProc.Play (), LpProc.Pause (), LpProc.Shuffle (), LpProc.ShuffleOff (), LpProc.Repeat (), LpProc.RepeatOff ()
		BEGIN
			MESSAGE "(VH) \fN.AV.CONDITION\fN.GLOBAL.CONDITION\fN.AVN.MODE\fN.AVN.IPOD.MUSIC.AVAILABLE" ;
		END
	IF N.CID = @N_TAG_PLAY_TRACK_NUMBER_ID THEN ! LpProc.PlayTrackNumber ()
		BEGIN
			MESSAGE "(VH) \fN.AV.CONDITION\fN.GLOBAL.CONDITION\fN.AVN.TRACK.NUMBER" ;
		END
	IF N.CID = @N_TAG_VOICE_MEMO_ID THEN ! LpProc.VoiceMemo ()
		BEGIN
			MESSAGE "(VH) \fN.AVN.VOICEMEMO.AVAILABLE" ;
		END
	IF N.CID = @N_TAG_PRIM_USB_MUSIC_ID THEN ! Lpproc.USBMusic ()
		BEGIN
			MESSAGE "(VH) \fN.AVN.USB.AVAILABLE\fN.AVN.USB.LOADING\fN.AVN.USB.MUSIC.AVAILABLE\fN.AVN.IPOD.AVAILABLE\fN.AVN.IPOD.LOADING\fN.AVN.IPOD.MUSIC.AVAILABLE" ;
		END
	IF N.CID = @N_TAG_VIDEO_ID THEN ! Lpproc.Video ()
		BEGIN
