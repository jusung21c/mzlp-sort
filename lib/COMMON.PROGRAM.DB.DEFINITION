			MESSAGE "(VH) \fN.AVN.USB.AVAILABLE\fN.AVN.USB.LOADING\fN.AVN.USB.MUSIC.AVAILABLE\fN.AVN.USB.VIDEO.AVAILABLE\fN.AVN.IPOD.AVAILABLE\fN.AVN.IPOD.LOADING\fN.AVN.IPOD.MUSIC.AVAILABLE\fN.AVN.MEDIA.LAST.MODE" ;
		END
	IF N.CID = @N_TAG_BT_AUDIO_ID THEN ! LpProc.BtAudio ()
