! LOCAL VR 시나리오 관리를 위한 FLAG 처리
JINIE Common.LocalVrCheck (NUMBER N.DIALOG)

	! LOCAL VR CHECK
	IF N.DIALOG = @N_DIALOG_LOCAL_VR_BT_VAD | N.DIALOG = @N_DIALOG_LOCAL_VR_BT_VAD_NUM_LOCAL |
		 N.DIALOG = @N_DIALOG_LOCAL_VR_MAINMENU_AM_FREQUENCY | N.DIALOG = @N_DIALOG_LOCAL_VR_MAINMENU_FM_FREQUENCY |
		 N.DIALOG = @N_DIALOG_LOCAL_VR_MAINMENU_SIRIUSXM_CHANNEL | N.DIALOG = @N_DIALOG_LOCAL_VR_NAVI_FIND_BIG_CATEGORY |
		 N.DIALOG = @N_DIALOG_LOCAL_VR_NAVI_VDE_LOCATION | N.DIALOG = @N_DIALOG_LOCAL_VR_NAVI_FIND_SERVER THEN
		BEGIN
			N.INNER.LOCALVR.FLAG := 1 ;
		END

	! LOCAL VR CHECK LOG
	IF N.INNER.LOCALVR.FLAG = 1 THEN
		BEGIN
			S.INNER.LOG.STRING ~= N.DIALOG ;
			LOG "LP LOG : Common.LocalVrCheck - N.INNER.LOCALVR.FLAG = 1, DIALOG = " + S.INNER.LOG.STRING ;
		END
EOP

JINIE Common.LocalVrHelpRejection ()
	! LOCAL VR
	IF N.INNER.LOCALVR.FLAG = 1 & $S.STT.RESULT.INTENTION[1][1] = "Help" THEN
		BEGIN
			CALL Post.Garbage () ;
		END
EOP

! Number Prompt 예외 처리
JINIE Common.NumberPromptException (NUMBER N.NUMBER.IN, STRING S.NUMBER.OUT)

	S.NUMBER.OUT ~= N.NUMBER.IN ;
EOP
